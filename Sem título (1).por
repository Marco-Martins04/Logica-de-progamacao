programa {
  funcao inicio() {
    inteiro valor 

    escreva("Digite seu valor:")
    leia(valor)

    se (valor > 10) 
      escreva("É MAIOR QUE 10!")
    se (valor < 10) {
       escreva("NÃO É MAIOR QUE 10!")
    }  
    se (valor == 10)
       escreva("O NÚMERO É IGUAL A 10!")
  

    escreva("\n===Fim do progama ===")

  }
}
